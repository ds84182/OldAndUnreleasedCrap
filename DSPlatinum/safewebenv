local function copy(t)
	local ret = {}
	for i,v in pairs(t) do
		if type(v) == "table" then
			ret[i] = copy(v)
		else
			ret[i] = v
		end
	end
	return ret
end
local env = {}
env._G = {}
env.pairs = pairs
env.ipairs = ipairs
env.next = next
env.sleep = sleep
env.math = copy(math)
env.table = copy(table)
env.rednet = copy(rednet)
env.coroutine = copy(coroutine)
return env
